<h2><%=current_user.username%>'s Car</h2>

<h3><%=@car.make%></h3>
<h3><%=@car.model%></h3>
<h3><%=@car.year%></h3>
<h3><%=@car.trim%></h3><br>
<p><a href="/cars/<%=@car.id%>/edit">Edit Car</a></p>
<p><a href="/cars">All Cars</a></p>

<form method="post" action="/cars/<%= @car.id %>/delete">
    <input type="hidden" name="_method" value="delete">
    <input type="submit" value="Delete Car">
</form>


